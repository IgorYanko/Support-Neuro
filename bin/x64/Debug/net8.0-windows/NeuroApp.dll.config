<?xml version="1.0" encoding="utf-8" ?>
<RunSettings>
	<RunConfiguration>
		<Debugging>true</Debugging>
		<DisableAppDomain>true</DisableAppDomain>
		<JustMyCode>false</JustMyCode>
		<OptimizeForPortability>false</OptimizeForPortability>
		<EnableSourceLinkSupport>true</EnableSourceLinkSupport>
	</RunConfiguration>
	<DataCollectionRunSettings>
		<DataCollectors>
			<DataCollector friendlyName="Code Coverage">
				<Configuration>
					<CodeCoverage>
						<ModulePaths>
							<Include>
								<ModulePath>.*</ModulePath>
							</Include>
						</ModulePaths>
					</CodeCoverage>
				</Configuration>
			</DataCollector>
		</DataCollectors>
	</DataCollectionRunSettings>
</RunSettings>